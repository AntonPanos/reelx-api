{
  "name": "reelx_api",
  "private": true,
  "version": "0.1.0",
  "description": "Api for reelx app",
  "keywords": [
    "Typescript",
    "Node"
  ],
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "build": "tsc",
    "test": "jest --detectOpenHandles",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "author": "Panos Antonellos",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "module-alias": "^2.2.3",
    "mongoose": "^7.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.3.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  },
  "_moduleAliases": {
    "@/config": "dist/config",
    "@/controllers": "dist/api/controllers",
    "@/interfaces": "dist/api/interfaces",
    "@/library": "dist/api/library",
    "@/middleware": "dist/api/middleware",
    "@/models": "dist/api/models",
    "@/routes": "dist/api/routes",
    "@/utils": "dist/api/utils",
    "@/*": "dist/*"
  }
}
